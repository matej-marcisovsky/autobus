{
  "name": "autobus",
  "version": "1.0.0",
  "description": "Autobus card game.",
  "main": "index.js",
  "watch": {
    "bundle": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx,less"
    }
  },
  "scripts": {
    "build": "tsc",
