  "bugs": {
    "url": "https://github.com/matej-marcisovsky/autobus/issues"
  },
  "homepage": "https://github.com/matej-marcisovsky/autobus#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/react": "^17.0.43",
    "jest": "^27.4.7",
    "npm-watch": "^0.11.0",
    "rollup": "^2.70.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "array-shuffle": "^3.0.0",
    "bulma": "^0.9.3",
    "classnames": "^2.3.1",
